<?xml version="1.0" encoding="utf-8" ?>
<module>
  <id>Shipstation.Fulfillment</id>
  <version>2.9.0</version>
  <platformVersion>2.9.0</platformVersion>
  <dependencies>
    <dependency id="VirtoCommerce.Core" version="2.9.0" />
  </dependencies>

  <title>Shipstation fulfillment</title>
  <description>
    Shipstation module allows to synchronize orders with ShipStation. That way new orders with shipments made in VirtoCommerce will become available in ShipStation admin and changes of shipment status will be synced with VirtoCommerce platform store.
  </description>
  <authors>
    <author>Virto Commerce</author>
  </authors>
  <owners>
    <owner>Virto Commerce</owner>
  </owners>
  <projectUrl>http://virtocommerce.com/</projectUrl>
  <iconUrl>Modules/$(Shipstation.Fulfillment)/Content/logo.png</iconUrl>
  <releaseNotes>First version.</releaseNotes>
  <copyright>Copyright © 2011-2015 Virto Commerce. All rights reserved</copyright>
  <tags>shipstation fulfillment</tags>

  <assemblyFile>Shipstation.FulfillmentModule.Web.dll</assemblyFile>
  <moduleType>Shipstation.FulfillmentModule.Web.Module, Shipstation.FulfillmentModule.Web</moduleType>
  <!--<styles>
		<directory virtualPath="$/Content/css" searchPattern="*.css" searchSubdirectories="true" />
	</styles>
  <scripts>
    <directory virtualPath="$/Scripts" searchPattern="*.js" searchSubdirectories="true" />
  </scripts>-->
  <permissions>
    <group name="Fulfillment">
      <permission id="fulfillment:manage" name="Manage fulfillment settings" description="Permission to manage shipstation fulfillment."/>
    </group>
  </permissions>
</module>
