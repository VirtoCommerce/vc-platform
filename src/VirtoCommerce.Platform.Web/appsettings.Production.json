{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "System": "Error",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "VirtoCommerce.Platform.Modules": "Debug",
        "VirtoCommerce.Platform.Web.Startup": "Information"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "WriteTo": [ "Console" ]
  }
}
