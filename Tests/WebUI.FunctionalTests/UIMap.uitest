&lt;script id="reviewTemplate" type="text/x-jquery-tmpl"&gt;
    &lt;div class="review"&gt;
        &lt;p class="helpful"&gt;
            (&lt;span id="positive_review_${Id}"&gt;${PositiveFeedbackCount}&lt;/span&gt; of &lt;span id="total_feedback_review_${Id}"&gt;${PositiveFeedbackCount + NegativeFeedbackCount}&lt;/span&gt; customers found this review helpful)
        &lt;/p&gt;
        &lt;div class="rating-block"&gt;
            &lt;div class="rating"&gt;
            &lt;/div&gt;
            &lt;div class="count"&gt;
                &lt;span class="num"&gt;${parseInt(Rating).toFixed(1)}&lt;/span&gt;
                ${RatingComment}									
            &lt;/div&gt;
        &lt;/div&gt;
        &lt;div class="author"&gt;
            &lt;p class="title"&gt;Review by &lt;span&gt;${Reviewer.NickName}&lt;/span&gt; {{if IsVerifiedBuyer}}&lt;span class="verified"&gt;Verified Buyer&lt;/span&gt;{{/if}}&lt;/p&gt;
            &lt;span class="date"&gt;${GetShortDateAndTime(CreatedDateTime)}&lt;/span&gt;
        &lt;/div&gt;
        &lt;div class="description"&gt;
            &lt;p&gt;${ReviewText.Text}&lt;/p&gt;
        &lt;/div&gt;
        &lt;div class="vote"&gt;
            &lt;p&gt;Was this review helpful?&lt;/p&gt;
            &lt;ul&gt;
                &lt;li&gt;&lt;a onclick="Reviews.vote('${Id}', true); return false;" href="#"&gt;Yes&lt;/a&gt;&lt;/li&gt;
                &lt;li class="divider"&gt;/&lt;/li&gt;
                &lt;li&gt;&lt;a onclick="Reviews.vote('${Id}', false); return false;" href="#"&gt;No&lt;/a&gt;&lt;/li&gt;
            &lt;/ul&gt;
        &lt;/div&gt;
        &lt;div id="review_feedback_${Id}" class="review-title"&gt;&lt;/div&gt;
        &lt;div class="feedback"&gt;
            &lt;ul&gt;
                &lt;li class="comment"&gt;&lt;a href="#" onclick="$('#comment_for_${Id}').show(); $('#report_abuse_for_${Id}').hide(); return false;"&gt;write a comment&lt;/a&gt;&lt;/li&gt;
                &lt;li class="divider"&gt;|&lt;/li&gt;
                &lt;li class="abuse"&gt;&lt;a href="#" onclick="$('#comment_for_${Id}').hide(); $('#report_abuse_for_${Id}').show(); return false;"&gt;report abuse&lt;/a&gt;&lt;/li&gt;
            &lt;/ul&gt;
            &lt;div class="feedback-comment" id="comment_for_${Id}"&gt;
                &lt;header&gt;
                    &lt;h3&gt;Your comment:&lt;/h3&gt;
                &lt;/header&gt;
&lt;form action="/store/en-us/api/review/comment" id="comment_${Id}" method="post"&gt;                    &lt;div class="control-group"&gt;
                        &lt;label for="AuthorName"&gt;Author: &lt;sup class="required"&gt;*&lt;/sup&gt;&lt;/label&gt;
                        &lt;input id="AuthorName" name="AuthorName" type="text" value="" /&gt;
                        &lt;span data-valmsg-replace="true" data-valmsg-for="AuthorName" class="field-validation-valid"&gt;&lt;/span&gt;
                    &lt;/div&gt;
                    &lt;div class="control-group"&gt;
                        &lt;label for="AuthorLocation"&gt;Location:&lt;/label&gt;
                        &lt;input id="AuthorLocation" name="AuthorLocation" type="text" value="" /&gt;
                    &lt;/div&gt;
                    &lt;div class="control-group"&gt;
                        &lt;label for="Comment"&gt;Comment: &lt;sup class="required"&gt;*&lt;/sup&gt;&lt;/label&gt;
                        &lt;textarea rows="10" cols="30" name="Comment"&gt;&lt;/textarea&gt;
                        &lt;span data-valmsg-replace="true" data-valmsg-for="Comment" class="field-validation-valid"&gt;&lt;/span&gt;
                    &lt;/div&gt;
                    &lt;input type="hidden" value="${Id}" name="Id" /&gt;
                    &lt;div class="button-group"&gt;
                        &lt;button type="button" value="Submit" onclick="Reviews.comment('${Id}');"&gt;
                            &lt;span&gt;Submit&lt;/span&gt;
                        &lt;/button&gt;
                        &lt;button type="button" value="Cancel" onclick="$('#comment_for_${Id}').hide(); return false;" class="btn cancel"&gt;
                            &lt;span&gt;Cancel&lt;/span&gt;
                        &lt;/button&gt;
                    &lt;/div&gt;
&lt;/form&gt;            &lt;/div&gt;
            &lt;div class="feedback-abuse" id="report_abuse_for_${Id}"&gt;
                &lt;header&gt;
                    &lt;h3&gt;Please let us know what kind of issue this is:&lt;/h3&gt;
                &lt;/header&gt;
&lt;form action="/store/en-us/api/review/reportabuse" id="abuse_${Id}" method="post"&gt;                    &lt;ul class="form-list"&gt;
                        &lt;li&gt;
                            &lt;label&gt;
                                &lt;input type="radio" value="profane" name="Reason"&gt;
                                Profanity
                            &lt;/label&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                            &lt;label for=""&gt;
                                &lt;input type="radio" value="wrong_product" name="Reason"&gt;
                                Wrong product
                            &lt;/label&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                            &lt;label&gt;
                                &lt;input type="radio" value="spam" name="Reason"&gt;
                                Spam
                            &lt;/label&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                            &lt;label&gt;
                                &lt;input type="radio" value="duplicate" name="Reason"&gt;
                                Duplicate
                            &lt;/label&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                            &lt;label&gt;
                                &lt;input type="radio" value="copyright" name="Reason"&gt;
                                Copyright violation
                            &lt;/label&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                            &lt;label&gt;
                                &lt;input type="radio" value="not_review" name="Reason"&gt;
                                Not a product review
                            &lt;/label&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                            &lt;label&gt;
                                &lt;input type="radio" value="image_issue" name="Reason"&gt;
                                Problem with image

                            &lt;/label&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                            &lt;label for=""&gt;
                                &lt;input type="radio" value="other" name="Reason"&gt;
                                Other

                            &lt;/label&gt;
                        &lt;/li&gt;
                        &lt;li&gt;
                            &lt;span data-valmsg-replace="true" data-valmsg-for="Reason" class="field-validation-valid"&gt;&lt;/span&gt;
                        &lt;/li&gt;
                    &lt;/ul&gt;
                    &lt;div class="control-group"&gt;
                        &lt;label for=""&gt;Email Address: &lt;sup class="required"&gt;*&lt;/sup&gt;&lt;/label&gt;
                        &lt;input type="text" maxlength="80" name="Email"&gt;
                        &lt;span data-valmsg-replace="true" data-valmsg-for="Email" class="field-validation-valid"&gt;&lt;/span&gt;
                    &lt;/div&gt;
                    &lt;div class="control-group"&gt;
                        &lt;label for=""&gt;Comments: &lt;sup class="required"&gt;*&lt;/sup&gt;&lt;/label&gt;
                        &lt;textarea rows="10" cols="30" name="Comment"&gt;&lt;/textarea&gt;
                    &lt;/div&gt;
                    &lt;input type="hidden" value="${Id}" name="Id" /&gt;
                    &lt;input type="hidden" value="True" name="IsReview" /&gt;
                    &lt;div class="button-group"&gt;
                        &lt;button type="button" value="Submit" onclick="Reviews.flag('${Id}');"&gt;
                            &lt;span&gt;Submit&lt;/span&gt;
                        &lt;/button&gt;
                        &lt;button type="button" value="Cancel" onclick="$('#report_abuse_for_${Id}').hide(); return false;" class="btn cancel"&gt;
                            &lt;span&gt;Cancel&lt;/span&gt;
                        &lt;/button&gt;
                    &lt;/div&gt;
&lt;/form&gt;            &lt;/div&gt;
        &lt;/div&gt;

        {{if TotalComments &gt; 0}}
        &lt;div id="show_all_comment_for_${Id}"&gt;
            &lt;p class="title"&gt;There are ${TotalComments} comments in total&lt;/p&gt;
            {{if Comments.length &lt; TotalComments }}
        &lt;a href="#" onclick="Reviews.showAllComments('${Id}'); return false;"&gt;Show all&lt;/a&gt;
            {{/if}}
        &lt;/div&gt;
        {{/if}}
    {{if Comments}}
        &lt;div id="comments_container_${Id}"&gt;
            {{tmpl(Comments) "#commentTemplate"}}
        &lt;/div&gt;
        {{/if}}
