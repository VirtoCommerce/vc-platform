    100% {
        stroke-dashoffset: 0
    }
}
@-webkit-keyframes bump {
    0%, 100% {
        -webkit-transform: none;
        transform: none
    }
    50% {
        -webkit-transform: scale3d(1.1, 1.1, 1);
        transform: scale3d(1.1, 1.1, 1)
    }
}
@keyframes bump {
    0%, 100% {
        -webkit-transform: none;
        transform: none
    }
    50% {
